{
  "@opentermsarchive/engine": {
    "recorder": {
      "versions": {
        "storage": {
          "git": {
            "snapshotIdentiferTemplate": "https://github.com/cloudresiliencelab/german-snapshots/commit/%SNAPSHOT_ID",
            "repository": "git@github.com:cloudresiliencelab/german-versions.git",
            "path": "./data/versions"
          }
        }
      },
      "snapshots": {
        "storage": {
          "git": {
            "repository": "git@github.com:cloudresiliencelab/german-snapshots.git",
            "path": "./data/snapshots"
          }
        }
      }
    },
    "fetcher": {
      "language": "en"
    },
    "notifier": {
      "sendInBlue": {
        "updatesListId": 0,
        "updateTemplateId": 0
      }
    },
    "reporter": {
      "type": "github",
      "repositories": {
        "declarations": "cloudresiliencelab/german-declarations",
        "versions": "cloudresiliencelab/german-versions",
        "snapshots": "cloudresiliencelab/german-snapshots"
      }
    },
    "dataset": {
      "title": "german",
      "versionsRepositoryURL": "https://github.com/cloudresiliencelab/german-versions"
    },
    "collection-api": {
      "port": 3000,
      "basePath": "/collection-api"
    },
    "logger": {
      "timestampPrefix": false
    }
  }
}
